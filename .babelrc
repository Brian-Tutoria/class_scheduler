{
  "presets": [
    [
      "@babel/env",
      {
        "corejs": 2,
        "modules": false,
        "forceAllTransforms": true,
        "targets": {
          "browsers": [ "> 1%", "last 10 versions"]
        },
        "useBuiltIns": "entry"
      },
      "es2015"
    ],
    "@babel/react"
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-proposal-class-properties", 
    [
      "transform-es2015-modules-commonjs",
      {
        "spec": true
      }
    ]
  ]
}
